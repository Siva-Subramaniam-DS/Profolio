{
  "name": "myportfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:security": "eslint . --ext .js,.jsx --config eslint.security.config.js",
    "preview": "vite preview",
    "start": "npx serve dist -s -l $PORT",
    "deploy": "npm run build && powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run lint:security && npm run security:audit",
    "test:security": "echo 'Running security tests...' && npm run security:check",
    "build:secure": "npm run security:check && npm run build",
    "postinstall": "npm rebuild rollup || true"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "framer-motion": "^12.15.0",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  }
}
